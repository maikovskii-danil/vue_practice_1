{
  "name": "vue-ts-applications-list",
  "version": "1.0.0+no-semver",
  "prettier": "@maikovskii-danil/prettier-config-vue",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "prestart": "npm clean-install --fund false",
    "start": "vite",
    "prebuild": "npm clean-install --fund false",
    "build": "npm run buildOnly",
    "buildOnly": "vite build",
    "preview": "vite preview",
    "format": "prettier --write --ignore-unknown src/",
    "prettierCheck": "prettier --check --ignore-unknown src/",
    "prelint": "npm clean-install --fund false --audit false",
    "lint": "npm run lintOnly",
    "lintOnly": "eslint --config ./eslint.config.js",
    "postlintOnly": "echo 'lint is done'"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.5",
    "@vueuse/core": "^13.2.0",
    "currency.js": "^2.0.4",
    "pinia": "^3.0.2",
    "tailwindcss": "^4.1.7",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@maikovskii-danil/prettier-config-vue": "^3.0.0",
    "@maikovskii-danil/eslint-config-js": "^3.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.31",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "~9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "globals": "~16.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "typescript": "~5.8.0",
    "typescript-eslint": "~8.34.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  },
  "engines": {
    "node": "^22.16.0",
    "npm": "^10.9.2"
  },
  "os": [
    "darwin"
  ],
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "error"
    }
  }
}